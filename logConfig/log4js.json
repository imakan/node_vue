{
  "appenders": [
    {
      "type":"console",
      "category":"console"
    },
    {
      "category":"log_file",
      "type": "dateFile",
      "filename": "./logs/access.log",
      "pattern": "-yyyy-MM-dd",
      "maxLogSize": 1024,
      "backups":4,
      "alwaysIncludePattern": true
    }
  ],
  "replaceConsole": true,
  "levels":
  {
    "log_file":"ALL",
    "console":"ALL",
    "log_date":"ALL"
  }
}
